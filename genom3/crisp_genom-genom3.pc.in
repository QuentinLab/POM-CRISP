# pkg-config file for crisp_genom interface and codels library
#
prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
datarootdir=@datarootdir@
idldir=@datadir@/idl

Name: crisp_genom-genom3
Description: crisp_genom interface and codels library
Version: @PACKAGE_VERSION@
Requires: openrobots-idl >= 1.1, genom3 >= 2.99.26, crisp-asn1 crisp asn1_pom_conversions cdff_types pom-base
Cflags: -I${includedir} -I${idldir}
Libs: -L${libdir} -lcrisp_genom_codels
